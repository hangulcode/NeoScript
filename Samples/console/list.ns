import dump;

fun Fun_List()
{
	var A = [[1, 2, 3], [4, 5, 6]];
	var B = [1, 2, 3];
	var B1 = [[1, 2], [3, 4], [5, 6]];
	
	print("broadcast : " .. dump.object(A.broadcast(B)));
	print("multiply : " .. dump.object(A.multiply(B1)));
	print("dot : " .. dump.object(A.dot(A)));
	print("dot : " .. dump.object(B.dot(B)));
	print("sum : " .. dump.object(B.sum()));
}

Fun_List();
