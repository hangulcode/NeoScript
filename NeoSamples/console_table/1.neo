
export fun main()
{
	var table = {};
	table[5] = 3;
	table[6] = {};
	table[6][2] = 999;
	table[6][3] = 888;
	table["b2"] = 57;
	table["c3"] = "Hello Neo Script...";
	table["c4"] = "system";

	for (var i = 0; i < 10; i++)
	{
		if(i % 2 == 0)
			table[i] = {};
		else
			table[i] = i;
	}

	foreach(var a, b in table)
	{
		system.print(a ..":".. b .. "\n");
		sleep(200);
	}
}
