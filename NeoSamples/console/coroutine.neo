var cr;
fun sub(var param1)
{
	var cr2 = cr;
	for(var i in 0, 5, 1)
	{
		print("Coroutine " .. i);
		sleep(200);
		yield;
	}
}

fun test_multi()
{
	cr = coroutine_create(sub);

	for(var i in 0, 10, 1)
	{
		var state = coroutine_status(cr);
		if(state == "suspended")
		{
			coroutine_resume(cr, 2);
		}
		print("Main " .. state .. i);
		sleep(200);
	}
}


export fun test()
{
	test_multi();
}
