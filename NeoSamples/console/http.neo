print("http request ... http://example.com/");

var tm = clock();

fun Result(var ok, var result)
{
	if(ok == true)
		print("Success Time : " .. (clock() - tm));
	else
		print("Fail Time : " .. (clock() - tm));

	print(result);
}

var async = aysnc_create();
async.get("http://example.com/", Result);
if(false == async.wait(5000))
	print("Time Out : " .. (clock() - tm));
async.close();
